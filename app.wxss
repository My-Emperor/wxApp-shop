/**app.wxss**/
@import "./style/iconfont.wxss";

/*
  初始化样式  小程序不支持通配符  
*/
page,
view,
text,
swiper,
swiper-item,
image,
navigator{
  padding: 0;
  margin: 0;
  box-sizing: border-box;
}

page{
  /* 自定义颜色 */
  --themeColor:#eb4450;
  /* 定义统一的字体大小 */
  /* 1px = 2rpx */
  /* 14px = 28rpx */
  font-size:28rpx;
}
image{
  width: 100%;
}
